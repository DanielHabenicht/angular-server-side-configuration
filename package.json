{
  "name": "angular-server-side-configuration",
  "version": "0.2.1",
  "description": "Configure an angular application on the server",
  "main": "./src/main.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "clean": "rimraf .nyc_output coverage src/*.d.ts src/*.js",
    "pretest": "npm run clean",
    "test": "nyc mocha ./src/*.spec.ts",
    "prepack": "tsc",
    "postpublish": "npm run clean"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.spec.ts",
      "src/main.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyubisation/angular-server-side-configuration.git"
  },
  "keywords": [
    "angular",
    "configuration",
    "server",
    "server-side"
  ],
  "author": "kyubisation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kyubisation/angular-server-side-configuration/issues"
  },
  "homepage": "https://github.com/kyubisation/angular-server-side-configuration#readme",
  "dependencies": {
    "esm": "^3.0.84"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  }
}
